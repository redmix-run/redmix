{
  "name": "@cedarjs/auth-supabase-middleware",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git",
    "directory": "packages/auth-providers/supabase/middleware"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "cjsWrappers"
  ],
  "scripts": {
    "build": "tsx ./build.mts",
    "build:pack": "yarn pack -o cedar-auth-supabase-middleware.tgz",
    "build:types": "tsc --build --verbose ./tsconfig.build.json",
    "build:types-cjs": "tsc --build --verbose ./tsconfig.cjs.json",
    "check:attw": "yarn attw -P",
    "check:package": "concurrently npm:check:attw yarn:publint",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@cedarjs/auth-supabase-api": "workspace:*",
    "@cedarjs/web": "workspace:*",
    "@supabase/ssr": "0.5.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.16.4",
    "@cedarjs/api": "workspace:*",
    "@cedarjs/auth": "workspace:*",
    "@cedarjs/framework-tools": "workspace:*",
    "@cedarjs/graphql-server": "workspace:*",
    "@types/aws-lambda": "8.10.145",
    "concurrently": "8.2.2",
    "publint": "0.3.11",
    "ts-toolbelt": "9.6.0",
    "tsx": "4.19.3",
    "typescript": "5.6.2",
    "vitest": "2.1.9"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
